# Example environment variables for Social Impact Tracker
# Copy this file to .env and update with your values

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=True

# Database
DATABASE_PATH=./data/social_impact.db

# Frontend
API_URL=http://localhost:8000

# Compression
COMPRESSION_ENABLED=True

# Logging
LOG_LEVEL=INFO
